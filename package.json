{
  "name": "hapiline",
  "version": "0.12.1",
  "adonis-version": "4.1.0",
  "description": "A habit tracker",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:fix": "eslint . --fix",
    "prettify": "pretty-quick",
    "prettify:staged": "npm run prettify -- --staged",
    "api:start": "adonis serve --dev --polling",
    "api:test": "adonis test --bail",
    "api:test:specific": "adonis test --grep",
    "frontend:dev": "webpack --mode development",
    "frontend:prod": "webpack --mode production --config prod.webpack.config.js",
    "e2e:desktop": "./node_modules/.bin/cypress open --browser chromium --config-file cypress/config/cypress.desktop.json",
    "e2e:desktop:headless": "./node_modules/.bin/cypress run --browser chromium --config-file cypress/config/cypress.desktop.json --headless",
    "e2e:mobile": "./node_modules/.bin/cypress open --browser chromium --config-file cypress/config/cypress.mobile.json --env 'device=mobile'",
    "e2e:mobile:headless": "./node_modules/.bin/cypress run --browser chromium --config-file cypress/config/cypress.mobile.json --env 'device=mobile' --headless",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "env:validate:dev": "ts-node -P ./tsconfig.scripts.json ./scripts/validate_env.ts",
    "env:validate:all": "npm run env:validate:dev -- --with-prod",
    "manifest:regenerate": "ts-node -P ./tsconfig.scripts.json ./scripts/regenerate_manifest.ts",
    "db:reseed": "./run.sh adonis migration:refresh && ./run.sh adonis seed",
    "ngrok": "ngrok http localhost:3333"
  },
  "author": "Bartosz Gordon",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.1.0",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/drive": "^1.0.4",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/http-logger": "^1.0.0",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/mail": "^3.0.10",
    "@adonisjs/persona": "^1.0.5",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/vow": "^1.0.17",
    "@bugsnag/js": "^7.1.1",
    "@bugsnag/plugin-react": "^7.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@prisma/client": "^2.0.0-beta.5",
    "@reach/alert": "^0.6.4",
    "@reach/alert-dialog": "^0.6.4",
    "@reach/dialog": "^0.6.4",
    "@reach/menu-button": "^0.10.4",
    "@reach/skip-nav": "^0.10.5",
    "@reach/tabs": "^0.8.6",
    "@reach/visually-hidden": "^0.6.2",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.17",
    "@types/react-beautiful-dnd": "^11.0.4",
    "@types/react-dom": "^16.9.4",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.3",
    "adonis-acl": "^1.1.1",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.0",
    "date-fns": "^2.8.1",
    "dotenv-webpack": "^1.7.0",
    "execa": "^3.4.0",
    "fast-deep-equal": "^3.1.1",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "pg": "^7.16.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-polymorphic-box": "^1.0.4",
    "react-query": "^1.5.2",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "tinykeys": "^1.0.6",
    "ts-storage": "^5.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.5",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/cypress-axe": "^0.8.0",
    "@types/lodash.omit": "^4.5.6",
    "@types/testing-library__cypress": "^5.0.5",
    "@types/yup": "^0.29.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.0",
    "cypress": "^4.12.1",
    "cypress-axe": "^0.8.1",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-bgord": "^2.2.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "react-cosmos": "^5.0.7",
    "style-loader": "^1.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-webpack-plugin": "^5.1.3",
    "yup": "^0.29.1"
  },
  "autoload": {
    "App": "./app"
  },
  "eslintConfig": {
    "extends": "bgord",
    "rules": {
      "no-sparse-arrays": 0,
      "jest/valid-expect": 0,
      "no-unused-expressions": 1
    }
  }
}
